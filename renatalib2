function bounceOf(ob3,ob4){
    if (ob3.x - ob4.x < ob3.width/2 + ob4.width/2
        && ob4.x - ob3.x < ob4.width/2 + ob1.width/2) {
      ob3.velocityX = -ob3.velocityX;
      ob4.velocityX = -ob4.velocityX;
        }
       if( ob3.y - ob4.y < ob4.height/2 + ob3.height/2
        && ob4.y - ob3.y < ob4.height/2 + ob3.height/2) {
        ob3.velocityY = -ob3.velocityY;
        ob4.velocityY = -ob4.velocityY;
    }
    
  }